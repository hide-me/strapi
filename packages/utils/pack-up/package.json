{
  "name": "@strapi/pack-up",
  "version": "4.13.3",
  "description": "Simple tools for creating interoperable CJS & ESM packages.",
  "keywords": [
    "strapi",
    "package",
    "cjs",
    "esm",
    "bundling",
    "build",
    "interoperable",
    "tools"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/strapi/strapi.git",
    "directory": "packages/utils/pack-up"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "Strapi Solutions SAS",
    "email": "hi@strapi.io",
    "url": "https://strapi.io"
  },
  "maintainers": [
    {
      "name": "Strapi Solutions SAS",
      "email": "hi@strapi.io",
      "url": "https://strapi.io"
    }
  ],
  "bin": {
    "pkg-utils": "./bin/pack-up.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "clean": "run -T rimraf ./dist",
    "prepublishOnly": "yarn clean"
  },
  "dependencies": {
    "@vitejs/plugin-react": "4.0.4",
    "boxen": "5.1.2",
    "chalk": "4.1.2",
    "commander": "8.3.0",
    "ora": "5.4.1",
    "vite": "4.4.9"
  },
  "devDependencies": {
    "typescript": "5.2.2"
  },
  "engines": {
    "node": ">=16.0.0 <=20.x.x",
    "npm": ">=6.0.0"
  }
}
